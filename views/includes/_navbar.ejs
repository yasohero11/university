  <script>
       $(document).ready(() => {
                $(window).resize(function () {
                if (($(window).width() <= 766)) {
                $("#show-on-col").show();
                $("#hide-on-col").hide();
                } else {
                $("#show-on-col").hide();
                $("#hide-on-col").show();
                }
            });

            $(window).scroll(() => {
                if ($(window).scrollTop() > 50) {
                $("#mynav").addClass("sticky-top")
                } else {
                $("#mynav").removeClass("sticky-top")
                }
            })  
       })
  </script>