    function shortText(className){
      for (element of $(className)){
            let words =  element.innerHTML.split(" ")
            let text = ""            
           if(words.length >= 20){ 
            for(let i = 0 ; i < 20  ; i++)
              text = text +" "+ words[i]

            element.innerHTML = text + " ...."
           }
        }
    }
    $(document).ready(() => {
        
        shortText(".card-text")
        shortText(".newsText")
        
        $("#testimonial-slider").owlCarousel({
         loop:true,
         margin:20,
         responsiveClass:true,
         navText: ['<i class="fas fa-chevron-circle-left"></i>','<i class="fas fa-chevron-circle-right"></i>'],
         nav:true,                  
         responsive:{
            760:{
                items:2,
                nav:true   
            },
            0:{
                items:1,
                nav:true
            },
            1000:{
                items:3,
                nav:true,
                loop:false
            }
         } 

      
        });
    

    db.collection("news") .onSnapshot(function(snapshot) {

      snapshot.docChanges().forEach(function(change) {
          if (change.type === "modified") {
            console.log("en")
              let news =  $("#" + change.doc.id)
              let data = change.doc.data()
             
              news.find(".newsTitle").text(data.title)             
              news.find(".newsText").text(data.text)
              news.find(".img-fluid").attr("src" , "" + data.imagePath) 
          }
      });
      });
      
    })


  </script>
 